{
  "name": "@doomberg-terminal/sidepanel",
  "version": "0.0.1",
  "description": "chrome extension sidepanel",
  "private": true,
  "sideEffects": true,
  "files": [
    "dist/**"
  ],
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "pnpm run clean && tsc --noEmit && vite build",
    "build:watch": "cross-env __DEV__=true node --max-old-space-size=10000 ../../node_modules/vite/bin/vite.js build -w --mode development",
    "dev": "pnpm  build:watch",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "pnpm lint --fix",
    "prettier": "prettier . --write --ignore-path ../../.prettierignore",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@fluentui-contrib/react-chat": "^0.1.11",
    "@fluentui/react-components": "^9.55.1",
    "@fluentui/react-icons": "^2.0.262",
    "@google/genai": "^1.6.0",
    "@huggingface/transformers": "3.5.0",
    "@doomberg-terminal/shared": "workspace:*",
    "@types/d3-array": "^3.2.1",
    "@types/d3-scale": "^4.0.8",
    "@types/d3-time-format": "^4.0.3",
    "@types/react-window": "^1.8.8",
    "@visx/axis": "^3.12.0",
    "@visx/curve": "^3.12.0",
    "@visx/delaunay": "^3.12.0",
    "@visx/event": "^3.12.0",
    "@visx/grid": "^3.12.0",
    "@visx/group": "^3.12.0",
    "@visx/heatmap": "^3.12.0",
    "@visx/mock-data": "^3.12.0",
    "@visx/responsive": "^3.12.0",
    "@visx/scale": "^3.12.0",
    "@visx/shape": "^3.12.0",
    "@visx/text": "^3.12.0",
    "@visx/threshold": "^3.12.0",
    "@visx/tooltip": "^3.12.0",
    "@visx/xychart": "^3.12.0",
    "@xstate/react": "^4.1.3",
    "d3-array": "^3.2.4",
    "d3-scale": "^4.0.2",
    "d3-time-format": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "dom-to-semantic-markdown": "^1.5.0",
    "dompurify": "^3.2.6",
    "react-markdown": "^10.1.0",
    "react-range": "^1.10.0",
    "react-window": "^1.8.11",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.1",
    "remark-rehype": "^11.1.2",
    "xstate": "^5.18.2"
  },
  "devDependencies": {
    "@doomberg/hmr": "workspace:*",
    "@doomberg/tsconfig": "workspace:*"
  }
}
